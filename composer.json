{
  "name": "codinglabsau/yolo",
  "description": "YOLO infrastructure cli tool for deploying Laravel apps",
  "homepage": "https://github.com/codinglabsau/yolo",
  "license": "proprietary",
  "authors": [
    {
      "name": "Steve Thomas",
      "email": "steve@codinglabs.com.au",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.3",
    "aws/aws-sdk-php": "^3.324",
    "guzzlehttp/guzzle": "^7.9",
    "illuminate/container": "^10|^11|^12",
    "illuminate/filesystem": "^10|^11|^12",
    "illuminate/support": "^10|^11|^12",
    "joetannenbaum/chewie": "^0.1.8",
    "symfony/console": "^7.1",
    "symfony/yaml": "^7.1",
    "vlucas/phpdotenv": "^5.6"
  },
  "require-dev": {
    "laravel/pint": "^1.24",
    "pestphp/pest": "^3.7",
    "phpstan/phpstan": "^1.12",
    "spatie/ray": "^1.41"
  },
  "autoload": {
    "psr-4": {
      "Codinglabs\\Yolo\\": "src"
    }
  },
  "bin": [
    "yolo"
  ],
  "scripts": {
    "fix": "vendor/bin/php-cs-fixer fix"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}

{
  "name": "codinglabsau/yolo",
  "description": "YOLO infrastructure cli tool for deploying Laravel apps",
  "homepage": "https://github.com/codinglabsau/yolo",
  "license": "proprietary",
  "authors": [
    {
      "name": "Steve Thomas",
      "email": "steve@codinglabs.com.au",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.3",
    "aws/aws-sdk-php": "^3.324",
    "guzzlehttp/guzzle": "^7.9",
    "illuminate/container": "^10|^11",
    "illuminate/filesystem": "^10|^11",
    "illuminate/support": "^10|^11",
    "laravel/prompts": "^0.3.1",
    "spatie/ray": "^1.41",
    "symfony/yaml": "^7.1",
    "vlucas/phpdotenv": "^5.6"
  },
  "require-dev": {
    "codinglabsau/php-styles": "dev-main",
    "phpstan/phpstan": "^1.12"
  },
  "autoload": {
    "psr-4": {
      "Codinglabs\\Yolo\\": "src"
    }
  },
  "bin": [
    "yolo"
  ],
  "scripts": {
    "fix": "vendor/bin/php-cs-fixer fix"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
